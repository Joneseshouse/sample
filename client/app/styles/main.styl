.drop-zone-file-name
	color: #444444

.preview-image
	max-width: 100%

.ReactModal__Overlay
	z-index: 20 !important

.pagination-wrapper td
	border-bottom: 2px solid #DDDDDD
	padding: 0 !important
	padding-left: 5px !important
	padding-right: 5px !important
	padding-top: 5px !important

.Select-control,.Select-input
	height: 30px

.Select-value
	line-height: 30px!important
	font-size: 12px

.custom-modal-footer .submit
	text-align: right


.custom-modal-footer .cancel
	text-align: left

.main-menu .active
	background-color: #cccccc !important

.list-group-item.active-lv1
	z-index: 2
	color: #fff
	background-color: #337ab7
	border-color: #337ab7

.heading
	background-color: #337AB7
	color: white
	height: 44px

.heading-item
	line-height: 56px
	float: right

.heading-logo
	font-size: 20px
	line-height: 44px
	padding-left: 10px
	padding-right: 10px

.footer
	border-top: solid 2px #FF9800
	background-color: #337AB7
	color: white
	padding: 10px

.footer a
	color: white

.main-right-dropdown
	position: relative
	float: right
	display: table
	top: 13px
	right: 15px

.spinner-container
	position: fixed
	top: 0
	left: 0
	width: 100%
	height: 100%
	cursor: wait
	text-align: center
	z-index: 999999999

.spinner-sub-container
	width: 100%
	height: 45px
	padding-top: 5px
	background-color: white
	opacity: 0.3

.field-error
	font-size: 12px
	color: red
	margin-top: 5px

.breadcrumb-container
	background-color: #EFEFEF
	line-height: 40px
	padding-left: 10px
	font-weight: bold

.checkbox label
	line-height: 21px

.Select-control, .Select-input
	height: 32px

.DateInput
	z-index: 4 !important
	padding: 0 !important
	font-size: 12px !important

.SingleDatePickerInput .DateInput
	width: calc(100% - 50px) !important
.SingleDatePickerInput__clear-date
	padding: 0 !important


.DateRangePicker, .DateRangePickerInput
	width: 100% !important

.DateRangePickerInput .DateInput
	width: calc(50% - 30px) !important

.DateRangePickerInput__clear-dates
	padding: 0 !important
.DateRangePicker__picker
	z-index: 9 !important
.SingleDatePicker__picker
	z-index: 9 !important
.Select-menu-outer
	z-index: 9 !important
.SingleDatePicker
	width: 100%
.border-bottom
	border-bottom: 1px solid #DDDDDD

.status-table
	border: 1px solid #DDDDDD
	borderCollapse: collapse

.status-table td
	border: 1px solid #DDDDDD
	borderCollapse: collapse



.tooltip1
    position: relative

.tooltip1 .tooltiptext1
    visibility: hidden
    width: 120px
    background-color: rgba(0, 0, 0, 0.7)
    color: #fff
    text-align: center
    padding: 2px
    border-radius: 6px
    position: absolute
    z-index: 1

.tooltip1:hover .tooltiptext1
    visibility: visible;

.delivery-note-table td, .delivery-note-table th
	padding: 2px !important;

.fixed-footer
	position: fixed
	bottom: 0
	height: 52px
	width: 100%
	margin-left: -1px

.cart-checkbox
	width: 30px
.cart-stt
	width: 40px

@media print
	.printable
		display: block
		position: absolute
		top: 0 !important
		left: 0 !important
		right: 0 !important
	.non-printable
		display: none
	#content-wrapper-parent
		left: 0 !important
		right: 0 !important

@page
    size: auto
    margin-top: 0
    margin-left: 10mm
    margin-right: 10mm

.noselect
	-webkit-touch-callout: none /* iOS Safari */
	-webkit-user-select: none /* Chrome/Safari/Opera */
	-khtml-user-select: none /* Konqueror */
	-moz-user-select: none /* Firefox */
	-ms-user-select: none /* Internet Explorer/Edge */
	user-select: none /* Non-prefixed version, currently not supported by any browser */

.chat
    list-style: none;
    margin: 0;
    padding: 0;


.chat li
    margin-bottom: 10px;
    padding-bottom: 5px;
    border-bottom: 1px solid #B3A9A9;



.chat li.right .chat-body
    margin-right: 60px;

.chat li .chat-body p
    margin: 0;
    color: #000;

.panel .slidedown .glyphicon, .chat .glyphicon
    margin-right: 5px;

.panel-body
    overflow-y: scroll;
    height: 250px;

.text-admin
	color: #db0d06;

.message-box
	margin-top: 20px;
.description
	height: 300px
	overflow: auto
.chevron-icon
	float: right